{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "src": "^/login$",
      "dest": "/login.html"
    },
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "^/(?!.*\\.).*$",
      "dest": "/"
    },
    {
      "src": "^/(.*)",
      "dest": "/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
